<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Guardar</servlet-name>
        <servlet-class>Servlet.Guardar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InicioS</servlet-name>
        <servlet-class>Servlet.InicioS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesion</servlet-name>
        <servlet-class>Servlet.CerrarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Guardar2</servlet-name>
        <servlet-class>Servlet.Guardar2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InicioSA</servlet-name>
        <servlet-class>Servlet.InicioSA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesionA</servlet-name>
        <servlet-class>Servlet.CerrarSesionA</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Guardar</servlet-name>
        <url-pattern>/Save</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InicioS</servlet-name>
        <url-pattern>/ISesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesion</servlet-name>
        <url-pattern>/CerrarS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Guardar2</servlet-name>
        <url-pattern>/Save2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InicioSA</servlet-name>
        <url-pattern>/ISesionA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesionA</servlet-name>
        <url-pattern>/CerrarSA</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
